View(dados_ord)
ggplot(dados_ord, aes(x = reorder(species, -ocorrencias), y = ocorrencias)) +
geom_bar(stat = "identity", fill = "skyblue") +
geom_line(aes(y = acumulado * max(ocorrencias)), group = 1, color = "red") +
scale_y_continuous(sec.axis = sec_axis(~ . / max(dados_ord$ocorrencias), name = "Acumulado (%)")) +
coord_flip() +
theme_minimal() +
labs(x = "Espécie", y = "Ocorrências", title = "Gráfico de Pareto")
